<%= render "shared/title_bar" %>
<%= render "photos/partial_views/photo_content", photo: @photo, heading_1: true,
            size: 600 %>
<%= render "photos/partial_views/photo_user_options", photo: @photo %>
<%= render "photos/partial_views/photo_likes", photo: @photo %>
<%= render "photos/partial_views/list_of_photo_comments",
            photo: @photo, range: 0..-1, render_more: false %>
<% if current_user %>
  <%= render "photos/partial_views/new_photo_comment", photo: @photo %>
<% end %>
